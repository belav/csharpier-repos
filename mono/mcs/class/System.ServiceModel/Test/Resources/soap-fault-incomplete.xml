<Envelope
  {http://www.w3.org/2000/xmlns/}a="http://www.w3.org/2005/08/addressing"
  {http://www.w3.org/2000/xmlns/}s="http://www.w3.org/2003/05/soap-envelope"
>
  <Header>
    <Action {http://www.w3.org/2003/05/soap-envelope}mustUnderstand="1"
      >http://www.w3.org/2005/08/addressing/soap/fault</Action
    >
    <To {http://www.w3.org/2003/05/soap-envelope}mustUnderstand="1"
      >http://www.w3.org/2005/08/addressing/anonymous</To
    >
  </Header>
  <Body>
    <Fault>
      <Code>
        <Value>s:Sender</Value>
      </Code>
    </Fault>
  </Body>
</Envelope>
